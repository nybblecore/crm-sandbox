{
  "name": "crm-sandbox",
  "version": "1.0.1",
  "private": true,
  "proxy": "http://localhost:9000/",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "npm run db:migrate && node server/start.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nyc": {
    "all": true,
    "include": [
      "server/lib/**/*.js"
    ],
    "exclude": [
      "server/**/*spec.js",
      "server/test/**/*.js",
      "server/mock/**/*.js"
    ],
    "report-dir": "./coverage/server"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bfj": "6.1.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.5.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "axios": "^0.25.0",
    "dotenv": "^6.2.0",
    "dotenv-expand": "4.2.0",
    "ejs": "^3.1.5",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "fast-csv": "^4.3.6",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "fse": "^4.0.1",
    "http-proxy-middleware": "^0.19.2",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "js-yaml": "^4.1.0",
    "moment": "2.26.0",
    "moment-timezone": "^0.5.33",
    "serve-favicon": "^2.5.0",
    "sq-core": "^1.0.1",
    "underscore": "^1.10.2",
    "url-loader": "1.1.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
  }
}