{
  "name": "crm-sandbox",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:6001/",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start-api": "nodemon server/start.js --watch ./server --watch ./swishqube-core/cms --watch ./swishqube-core/server",
    "start-mock": "npm run start-api -- --mock=true",
    "start": "npm run db:migrate && node server/start.js",
    "start-dev": "node scripts/start.js",
    "build": "npm run server:test && node scripts/build.js && node scripts/build-web.js",
    "dist": "rm -rf dist && node scripts/build.js && node scripts/build-web.js && node scripts/dist.js",
    "build:dev": "node scripts/build.js && node scripts/build-web.js",
    "build:analyze": "export BUILD_ANALYZE=true && node scripts/build.js",
    "sync:ios:splash": "node scripts/build-ios.js  --env=production --mode=update-splash",
    "test": "node scripts/test.js --no-watch",
    "test:coverage": "node scripts/test.js --no-watch --coverage",
    "test:watch": "node scripts/test.js",
    "server:test": "mocha --file ./server/test/setup.js --exit --recursive ./server/lib",
    "server:api:test": "npm run db:migrate -- --env=test && npm run db:clean -- --env=test && export PORT=3000 && nyc --reporter=html --reporter=text mocha --full-trace  --file ./server/test/setup.js  --exit --recursive ./server/test/api",
    "server:test:coverage": "nyc --reporter=html --reporter=text mocha --file ./server/test/setup.js --exit --recursive ./server/lib",
    "server:test:watch": "mocha  -w --recursive ./server/lib",
    "test:all": "npm run server:test:coverage && npm run test:coverage",
    "test:local:all": "npm run server:api:test && npm run server:test:coverage && npm run test:coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nyc": {
    "all": true,
    "include": [
      "server/lib/**/*.js"
    ],
    "exclude": [
      "server/**/*spec.js",
      "server/test/**/*.js",
      "server/mock/**/*.js"
    ],
    "report-dir": "./coverage/server"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "animated-scroll-to": "^2.2.0",
    "aws-sdk": "^2.991.0",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "bfj": "6.1.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.5.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cognito-express": "^2.0.19",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^6.2.0",
    "dotenv-expand": "4.2.0",
    "ejs": "^3.1.5",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "fast-csv": "^4.3.6",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "fse": "^4.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "http-proxy-middleware": "^0.19.2",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "js-yaml": "^4.1.0",
    "material-bottom-nav": "^1.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "mobx": "^5.15.4",
    "mobx-react": "^5.4.3",
    "moment": "2.26.0",
    "moment-timezone": "^0.5.34",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.4.10",
    "razorpay": "^2.0.6",
    "react-google-recaptcha": "^2.1.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.3.0",
    "ip": "^1.1.5",
    "sq-core": "^1.0.1",
    "underscore": "^1.10.2",
    "url-loader": "1.1.2"
  },
